{
  "name": "ts-rolldown",
  "version": "0.0.0-development",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./a": {
      "source": "./src/a.ts",
      "import": "./dist/a.js",
      "require": "./dist/a.cjs",
      "default": "./dist/a.js"
    },
    "./b": {
      "source": "./src/b.ts",
      "import": "./dist/b.js",
      "require": "./dist/b.cjs",
      "default": "./dist/b.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pkg build --strict --check --clean",
    "clean": "rimraf dist",
    "typecheck": "tsc"
  },
  "browserslist": "extends @sanity/browserslist-config"
}
