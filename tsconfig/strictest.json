{
  "$schema": "https://json.schemastore.org/tsconfig",
  "description": "Stricter than strict",
  "extends": "./strict.json",
  "compilerOptions": {
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  }
}
