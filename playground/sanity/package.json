{
  "name": "sanity",
  "version": "3.37.2",
  "description": "A minimal repro of how sanity likes to bundle things",
  "license": "MIT",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/_exports/index.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./router": {
      "source": "./src/_exports/router.ts",
      "import": "./lib/router.mjs",
      "require": "./lib/router.js",
      "default": "./lib/router.js"
    },
    "./styled": {
      "source": "./src/_exports/styled.ts",
      "import": "./lib/styled.mjs",
      "require": "./lib/styled.js",
      "default": "./lib/styled.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "router": [
        "./lib/router.d.ts"
      ],
      "styled": [
        "./lib/styled.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "pkg-utils build --check --clean --strict",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "babel-plugin-react-compiler": "1.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "styled-components": "^6.1.19"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "styled-components": "*"
  }
}
