{
  "name": "multi-export",
  "version": "0.0.0-development",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "./side-effect*"
  ],
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./plugin": {
      "source": "./src/plugin.ts",
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs",
      "default": "./dist/plugin.js"
    },
    "./pure": {
      "source": "./src/pure.ts",
      "import": "./dist/pure.js",
      "require": "./dist/pure.cjs",
      "default": "./dist/pure.js"
    },
    "./side-effect": {
      "source": "./src/side-effect.ts",
      "import": "./dist/side-effect.js",
      "require": "./dist/side-effect.cjs",
      "default": "./dist/side-effect.js"
    },
    "./svelte": {
      "types": "./dist-svelte/index.d.ts",
      "svelte": "./dist-svelte/index.js"
    },
    "./no-side-effect": {
      "source": "./src/no-side-effect.ts",
      "import": "./dist/no-side-effect.js",
      "require": "./dist/no-side-effect.cjs",
      "default": "./dist/no-side-effect.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "plugin": [
        "./dist/plugin.d.ts"
      ],
      "pure": [
        "./dist/pure.d.ts"
      ],
      "side-effect": [
        "./dist/side-effect.d.ts"
      ],
      "no-side-effect": [
        "./dist/no-side-effect.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkg build --strict --clean && svelte-kit sync && svelte-package -i ./src/svelte -o dist-svelte && pkg check --strict",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@sanity/visual-editing": "^2.1.4",
    "@sveltejs/adapter-auto": "^3.2.1",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/package": "^2.3.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "dummy-side-effects": "workspace:*",
    "svelte": "^4.2.18",
    "vite": "^5.2.13"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@sveltejs/kit": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  }
}
