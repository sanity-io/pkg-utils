{
  "name": "ts",
  "version": "0.0.0-development",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./inline-inference": {
      "source": "./src/inline-inference.ts",
      "import": "./dist/inline-inference.js",
      "require": "./dist/inline-inference.cjs",
      "default": "./dist/inline-inference.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkg build --strict --check --clean",
    "clean": "rimraf dist",
    "typecheck": "tsc"
  }
}
