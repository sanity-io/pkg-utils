{
  "name": "@sanity/pkg-utils",
  "version": "10.1.1",
  "description": "Simple utilities for modern npm packages.",
  "keywords": [
    "sanity-io",
    "sanity",
    "npm",
    "package",
    "utilities",
    "build",
    "bundling"
  ],
  "homepage": "https://github.com/sanity-io/pkg-utils#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/pkg-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/pkg-utils.git",
    "directory": "packages/@sanity/pkg-utils"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/node/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pkg": "./bin/pkg-utils.js",
    "pkg-utils": "./bin/pkg-utils.js"
  },
  "files": [
    "bin",
    "dist",
    "tsconfig",
    "!dist/stats.html"
  ],
  "scripts": {
    "build": "run-s clean build:pkg check:pkg",
    "build:pkg": "tsx scripts/build.ts",
    "check:pkg": "tsx scripts/check.ts",
    "clean": "rimraf dist test/env/__tmp__",
    "extract": "sanity-tsdoc etl --outDir etc/api/@sanity/pkg-utils --tsconfig tsconfig.dist.json",
    "prepublishOnly": "pnpm run build",
    "pretest": "pnpm run clean && pnpm run build",
    "test": "vitest run",
    "typecheck": "tsc --build",
    "watch": "tsx scripts/watch.ts"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@microsoft/api-extractor": "^7.55.1",
    "@microsoft/tsdoc-config": "^0.18.0",
    "@optimize-lodash/rollup-plugin": "^6.0.0",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@sanity/browserslist-config": "^1.0.5",
    "@sanity/parse-package-json": "workspace:^",
    "@vanilla-extract/rollup-plugin": "^1.5.0",
    "browserslist": "^4.28.1",
    "cac": "^6.7.14",
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "empathic": "^2.0.0",
    "esbuild": "^0.27.0",
    "find-config": "^1.0.0",
    "get-latest-version": "^5.1.0",
    "git-url-parse": "^16.1.0",
    "globby": "^16.0.0",
    "jsonc-parser": "^3.3.1",
    "lightningcss": "^1.30.2",
    "mkdirp": "^3.0.1",
    "outdent": "^0.8.0",
    "prettier": "^3.7.3",
    "pretty-bytes": "^7.1.0",
    "prompts": "^2.4.2",
    "recast": "^0.23.11",
    "rimraf": "^6.1.2",
    "rolldown": "1.0.0-beta.52",
    "rolldown-plugin-dts": "0.18.1",
    "rollup": "^4.53.3",
    "rollup-plugin-esbuild": "^6.2.1",
    "rxjs": "^7.8.2",
    "treeify": "^1.1.0",
    "tsx": "^4.21.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@sanity/tsconfig": "workspace:^",
    "@types/babel__core": "^7.20.5",
    "@types/find-config": "^1.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "catalog:",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.7.1",
    "@types/treeify": "^1.0.3",
    "@typescript/native-preview": "catalog:",
    "babel-plugin-react-compiler": "1.0.0",
    "browserslist-to-esbuild": "2.1.1",
    "fs-extra": "^11.3.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "semver": "^7.7.3",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "babel-plugin-react-compiler": "*",
    "typescript": "5.8.x || 5.9.x"
  },
  "peerDependenciesMeta": {
    "babel-plugin-react-compiler": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  }
}
