{
  "name": "strict-legacy-fields",
  "version": "0.0.0",
  "private": true,
  "description": "Test package for strict legacy field checks",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {
    "./dist/index.js": "./dist/browser.js"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkg build --strict",
    "clean": "rimraf dist"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "devDependencies": {
    "@sanity/pkg-utils": "workspace:*"
  }
}
