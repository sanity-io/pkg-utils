{
  "name": "multi-export",
  "version": "0.0.0-development",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "./side-effect*"
  ],
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./plugin": {
      "source": "./src/plugin.ts",
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs",
      "default": "./dist/plugin.js"
    },
    "./pure": {
      "source": "./src/pure.ts",
      "import": "./dist/pure.js",
      "require": "./dist/pure.cjs",
      "default": "./dist/pure.js"
    },
    "./side-effect": {
      "source": "./src/side-effect.ts",
      "import": "./dist/side-effect.js",
      "require": "./dist/side-effect.cjs",
      "default": "./dist/side-effect.js"
    },
    "./no-side-effect": {
      "source": "./src/no-side-effect.ts",
      "import": "./dist/no-side-effect.js",
      "require": "./dist/no-side-effect.cjs",
      "default": "./dist/no-side-effect.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "plugin": [
        "./dist/plugin.d.ts"
      ],
      "pure": [
        "./dist/pure.d.ts"
      ],
      "side-effect": [
        "./dist/side-effect.d.ts"
      ],
      "no-side-effect": [
        "./dist/no-side-effect.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkg build --strict --check --clean",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@sanity/visual-editing": "^2.13.15",
    "dummy-side-effects": "workspace:*"
  }
}
