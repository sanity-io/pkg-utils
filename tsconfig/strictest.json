{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./strict.json",
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noImplicitReturns": true
  },
  "description": "Stricter than strict"
}
