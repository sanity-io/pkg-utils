{
  "name": "multi-export",
  "version": "0.0.0-development",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "./side-effect*"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "source": "./src/plugin.ts",
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs",
      "default": "./dist/plugin.js"
    },
    "./pure": {
      "types": "./dist/pure.d.ts",
      "source": "./src/pure.ts",
      "import": "./dist/pure.js",
      "require": "./dist/pure.cjs",
      "default": "./dist/pure.js"
    },
    "./side-effect": {
      "types": "./dist/side-effect.d.ts",
      "source": "./src/side-effect.ts",
      "import": "./dist/side-effect.js",
      "require": "./dist/side-effect.cjs",
      "default": "./dist/side-effect.js"
    },
    "./svelte": {
      "types": "./dist-svelte/index.d.ts",
      "svelte": "./dist-svelte/index.js"
    },
    "./no-side-effect": {
      "types": "./dist/no-side-effect.d.ts",
      "source": "./src/no-side-effect.ts",
      "import": "./dist/no-side-effect.js",
      "require": "./dist/no-side-effect.cjs",
      "default": "./dist/no-side-effect.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "plugin": [
        "./dist/plugin.d.ts"
      ],
      "pure": [
        "./dist/pure.d.ts"
      ],
      "side-effect": [
        "./dist/side-effect.d.ts"
      ],
      "no-side-effect": [
        "./dist/no-side-effect.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "run-s clean && pkg build --strict && svelte-kit sync && svelte-package -i ./src/svelte -o dist-svelte && pkg check --strict",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@sanity/visual-editing": "^1.7.1",
    "dummy-side-effects": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.3",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "svelte": "^4.2.12",
    "vite": "^5.1.6"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@sveltejs/kit": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  }
}
